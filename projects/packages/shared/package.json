{
  "name": "@monorepo/shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./result": {
      "import": "./src/result.ts",
      "types": "./src/result.ts"
    },
    "./domain-event": {
      "import": "./src/domain-event.ts",
      "types": "./src/domain-event.ts"
    },
    "./entity": {
      "import": "./src/entity.ts",
      "types": "./src/entity.ts"
    },
    "./repository": {
      "import": "./src/repository.ts",
      "types": "./src/repository.ts"
    },
    "./value-object": {
      "import": "./src/value-object.ts",
      "types": "./src/value-object.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/result.ts src/domain-event.ts src/entity.ts src/repository.ts src/value-object.ts --format esm --dts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^4.0.17"
  }
}
